;
; BIND data file for local loopback interface
;
$TTL    604800
@   IN  SOA ns.zonda.tw. root.proxy.zonda.tw. (
               1024     ; Serial
               3600	; Refresh
                600     ; Retry
              86400     ; Expire
                600 )   ; Negative Cache TTL
;
@      IN      NS  ns.zonda.tw.
@      IN      MX  10 ns.zonda.tw.

@      IN      A   111.222.111.222 
test   IN      A   11.22.33.101
web    IN      A   11.22.33.103
mail   IN      A   11.22.33.104
proxy  IN      A   11.22.33.106
haha   IN      A   11.22.33.107
